document.addEventListener("click",(function(t){var o=t.target.closest(".expand");o&&o.classList.toggle("expanded")}),!1),$((function(){$document=$(document),$html=$("html"),$carousel=$("#carousel"),$document.scroll((function(){$document.scrollTop()>99?$html.addClass("scroll"):$html.removeClass("scroll")})),$document.scroll(),$(".carousel").on("click",".arrow",(function(t){t.preventDefault();var o=$(".carousel").attr("data-show"),a=$(".carousel .dot").length-1;"back"==$(this).attr("data-direction")?o--:o++,o<0?o=a:o>a&&(o=0),$(".carousel").attr("data-show",o)})).on("click",".dot",(function(t){t.preventDefault(),$(".carousel").attr("data-show",$(this).attr("data-goto"))}))}));