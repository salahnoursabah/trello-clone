function initializeGoogleTagManager(e,t,a){window.dataLayer=window.dataLayer||[],function(a,n,g,i,o){a[i]=a[i]||[],a[i].push({"gtm.start":(new Date).getTime(),event:"gtm.js"});var r=n.getElementsByTagName(g)[0],m=n.createElement(g);m.async=!0,m.src="https://www.googletagmanager.com/gtm.js?id=GTM-PJ8M5SK&gtm_auth="+e+"&gtm_preview="+t+"&gtm_cookies_win=x",r.parentNode.insertBefore(m,r)}(window,document,"script","dataLayer")}